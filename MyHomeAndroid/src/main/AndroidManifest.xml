<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myhome">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/smart_house"
        android:label="@string/app_name"
        android:roundIcon="@drawable/smart_house"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyHome">
        <activity
            android:name=".HomeActivity1"
            android:label="@string/home_activity1" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/home_activity" />
        <activity
            android:name=".SetDateActivity"
            android:label="@string/set_date_activity" />
        <activity
            android:name=".SetTimeActivity"
            android:label="@string/set_time_activity" />
        <activity
            android:name=".TimeActivity"
            android:label="@string/time_activity" />
        <activity
            android:name=".SelectActivity"
            android:label="@string/select_activity" />
        <activity
            android:name=".ControlActivity"
            android:label="@string/control_activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>